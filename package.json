        "@babel/register": "^7.8.3",
        "@types/jest": "^25.2.3",
        "@types/react": "^16.9.19",
        "@types/react-dom": "^16.9.5",
        "@types/react-redux": "^7.1.16",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.15.0",
        "assets-webpack-plugin": "^3.9.10",
        "autoprefixer": "^9.7.4",
        "babel-jest": "^25.1.0",
        "babel-loader": "^8.0.6",
