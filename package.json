        "dotenv": "^8.2.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-redux": "^7.1.3",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0",
        "whatwg-fetch": "^3.0.0"
    },
